{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ALT-ADF"
		},
		"Altra_Test_Link_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Altra_Test_Link'",
			"defaultValue": "host=dnatestdb;port=1521;sid=attst1;user id=NEXTLYTRANSACTIONS_RO"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Claysys_Oracal_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Claysys_Oracal'"
		},
		"ALTADFKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ALTRA-KEYVAULT.vault.azure.net/"
		},
		"ls_transactiondb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ALTDevsql01RTTransactionsDev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ALT-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Altra_Test_Link_connectionString": {
						"value": "[parameters('Altra_Test_Link_connectionString')]"
					},
					"Claysys_Oracal_connectionString": {
						"value": "[parameters('Claysys_Oracal_connectionString')]"
					},
					"ALTADFKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ALTADFKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_transactiondb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_transactiondb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}