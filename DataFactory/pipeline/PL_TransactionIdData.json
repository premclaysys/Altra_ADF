{
	"name": "PL_TransactionIdData",
	"properties": {
		"activities": [
			{
				"name": "Get_Lookup",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "SELECT TRANSACTIONID\nFROM DNACUSTOM.VW_DIGITALBANKINGTRANSACTIONS\nWHERE SUBACCTNBR = (\n    SELECT MAX(SUBACCTNBR)\n    FROM DNACUSTOM.VW_DIGITALBANKINGTRANSACTIONS \n)",
						"partitionOption": "None",
						"convertDecimalToInteger": false,
						"queryTimeout": "02:30:00"
					},
					"dataset": {
						"referenceName": "ds_O_AltraServerConnectivity",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Getdata_fromlookup",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Get_Lookup",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "SELECT TRANSACTIONID, COUNT(*) AS TRANSACTION_COUNT\nFROM DNACUSTOM.VW_DIGITALBANKINGTRANSACTIONS\nWHERE TRANSACTIONID = '@{activity('Get_Lookup').output.TRANSACTIONID}'\nGROUP BY TRANSACTIONID\nORDER BY TRANSACTION_COUNT DESC;",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"dataset": {
						"referenceName": "ds_O_AltraServerConnectivity",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			}
		],
		"annotations": []
	}
}