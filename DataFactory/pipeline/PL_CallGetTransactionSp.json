						"sqlReaderQuery": "DECLARE @MaxTranId BIGINT;\n\nSELECT @MaxTranId = MAX(TransactionId)\nFROM Transactions\nWHERE TransactionFiscalDate = DATEADD(mm, -18, CONVERT(date, GETDATE()));\n\nWITH CTE AS\n(\n    SELECT  transactionId, TransactionFiscalDate, TransactionDateTime\n    FROM Transactions\n    WHERE transactionID <= @MaxTranId\n    AND TransactionFiscalDate <= DATEADD(mm, -18, CONVERT(date, GETDATE()))\n    ORDER BY TransactionID\n)\n\nSELECT COUNT(*) AS RecordCount\nFROM CTE;\n",
