{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_S_DigitalBankingTransactionRecord",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_S_TransactionHistory",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AccountNumber as decimal(22,0),",
				"          AccountTransactionNumber as decimal(22,0),",
				"          TransactionID as decimal(22,0),",
				"          TransactionFiscalDate as date,",
				"          TransactionDateTime as timestamp,",
				"          TransactionEffectiveDate as date,",
				"          TransactionTypeCode as string,",
				"          TransactionType as string,",
				"          TransactionStatusCode as string,",
				"          TransactionStatus as string,",
				"          TransactionDescription as string,",
				"          TransactionSortIndex as long,",
				"          PrincipalAmount as decimal(38,3),",
				"          PrincipalBalance as decimal(38,3),",
				"          LoanInterestAmount as decimal(38,3),",
				"          CURRRTXNSTATCD as string,",
				"          CHECKNBR as decimal(22,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: '\\nselect * from Transactions where TransactionDateTime <DateADd(mm,-18, CONVERT(date, GETDATE()))',",
				"     format: 'query') ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AccountNumber as string,",
				"          AccountTransactionNumber as string,",
				"          TransactionID as string,",
				"          TransactionFiscalDate as string,",
				"          TransactionDateTime as string,",
				"          TransactionEffectiveDate as string,",
				"          TransactionTypeCode as string,",
				"          TransactionType as string,",
				"          TransactionStatusCode as string,",
				"          TransactionStatus as string,",
				"          TransactionDescription as string,",
				"          TransactionSortIndex as string,",
				"          PrincipalAmount as string,",
				"          PrincipalBalance as string,",
				"          LoanInterestAmount as string,",
				"          CURRRTXNSTATCD as string,",
				"          CHECKNBR as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}