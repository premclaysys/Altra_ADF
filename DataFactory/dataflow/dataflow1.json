{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "azuresqldatasetsourse",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTablesink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TransferID as long,",
				"          UUID as long,",
				"          FromAccount as long,",
				"          FromSLType as string,",
				"          FromSuffix as string,",
				"          ToAccount as long,",
				"          ToSuffix as string,",
				"          Description as string,",
				"          StartDateTime as string,",
				"          NextTransferDate as string,",
				"          Amount as double,",
				"          TransferType as string,",
				"          ReoccuranceOption as string,",
				"          ReoccuranceDays as string,",
				"          ReoccuranceWeeks as string,",
				"          ReoccuranceDayofWeek as string,",
				"          EndEnum as integer,",
				"          EndOccurances as string,",
				"          EndDateTime as string,",
				"          TOSLType as string,",
				"          ReoccuranceMonthDay as string,",
				"          ReoccuranceMonths as string,",
				"          ReoccuranceMonthWeek as string,",
				"          ReoccuranceMonthDayofWeek as string,",
				"          ReoccuranceYearMonth as string,",
				"          ReoccuranceYearDay as string,",
				"          ReoccuranceYearWeek as string,",
				"          ReoccuranceYearDayofWeek as string,",
				"          StopDateTime as string,",
				"          Id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TransferID as long,",
				"          UUID as long,",
				"          FromAccount as long,",
				"          FromSLType as string,",
				"          FromSuffix as string,",
				"          FeeSuffix as string,",
				"          FeeAccountType as string,",
				"          ACHID as long,",
				"          BankName as string,",
				"          MaskedAccountNumber as string,",
				"          Description as string,",
				"          NextTransferDate as timestamp,",
				"          Amount as double,",
				"          TransferType as string,",
				"          ReoccuranceOption as string,",
				"          ReoccuranceDays as string,",
				"          ReoccuranceWeeks as string,",
				"          ReoccuranceDayofWeek as string,",
				"          ReoccuranceMonthDay as string,",
				"          ReoccuranceMonths as string,",
				"          ReoccuranceMonthWeek as string,",
				"          ReoccuranceMonthDayofWeek as string,",
				"          ReoccuranceYearMonth as string,",
				"          ReoccuranceYearDay as string,",
				"          ReoccuranceYearWeek as string,",
				"          ReoccuranceYearDayofWeek as string,",
				"          StartDateTime as timestamp,",
				"          EndEnum as integer,",
				"          EndOccurances as string,",
				"          EndDateTime as string,",
				"          Id as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TransferID,",
				"          UUID,",
				"          FromAccount,",
				"          FromSLType,",
				"          FromSuffix,",
				"          Description,",
				"          Amount,",
				"          TransferType,",
				"          ReoccuranceOption,",
				"          ReoccuranceDays,",
				"          ReoccuranceWeeks,",
				"          ReoccuranceDayofWeek,",
				"          ReoccuranceMonthDay,",
				"          ReoccuranceMonths,",
				"          ReoccuranceMonthWeek,",
				"          ReoccuranceMonthDayofWeek,",
				"          ReoccuranceYearMonth,",
				"          ReoccuranceYearDay,",
				"          ReoccuranceYearWeek,",
				"          ReoccuranceYearDayofWeek,",
				"          EndEnum,",
				"          EndOccurances,",
				"          EndDateTime",
				"     )) ~> sink1"
			]
		}
	}
}