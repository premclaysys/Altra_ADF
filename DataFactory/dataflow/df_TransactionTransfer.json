{
	"name": "df_TransactionTransfer",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_S_DigitalBankingConectivity",
						"type": "DatasetReference"
					},
					"name": "TransactionSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_S_DigitalBankingTransactionRecord",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          AccountNumber as string,",
				"          AccountTransactionNumber as string,",
				"          TransactionID as string,",
				"          TransactionFiscalDate as string,",
				"          TransactionDateTime as string,",
				"          TransactionEffectiveDate as string,",
				"          TransactionTypeCode as string,",
				"          TransactionType as string,",
				"          TransactionStatusCode as string,",
				"          TransactionStatus as string,",
				"          TransactionDescription as string,",
				"          TransactionSortIndex as string,",
				"          PrincipalAmount as string,",
				"          PrincipalBalance as string,",
				"          LoanInterestAmount as string,",
				"          CURRRTXNSTATCD as string,",
				"          CHECKNBR as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT * FROM Transaction_staging where TransactionID> 304473028',",
				"     format: 'query') ~> TransactionSource",
				"TransactionSource derive(TransactionDateTime = left(TransactionDateTime,23),",
				"          TransactionFiscalDate = left(TransactionFiscalDate,10),",
				"          AccountNumber = toLong(AccountNumber),",
				"          AccountTransactionNumber = toLong(AccountTransactionNumber),",
				"          TransactionID = toLong(TransactionID),",
				"          TransactionEffectiveDate = left(TransactionEffectiveDate,10),",
				"          PrincipalAmount = toLong(PrincipalAmount),",
				"          PrincipalBalance = toLong(PrincipalBalance),",
				"          LoanInterestAmount = toLong(LoanInterestAmount),",
				"          CHECKNBR = toLong(CHECKNBR)) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AccountNumber as decimal(22,0),",
				"          AccountTransactionNumber as decimal(22,0),",
				"          TransactionID as decimal(22,0),",
				"          TransactionFiscalDate as date,",
				"          TransactionDateTime as timestamp,",
				"          TransactionEffectiveDate as date,",
				"          TransactionTypeCode as string,",
				"          TransactionType as string,",
				"          TransactionStatusCode as string,",
				"          TransactionStatus as string,",
				"          TransactionDescription as string,",
				"          TransactionSortIndex as long,",
				"          PrincipalAmount as decimal(38,3),",
				"          PrincipalBalance as decimal(38,3),",
				"          LoanInterestAmount as decimal(38,3),",
				"          CURRRTXNSTATCD as string,",
				"          CHECKNBR as decimal(22,0)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          AccountNumber,",
				"          AccountTransactionNumber,",
				"          TransactionID,",
				"          TransactionFiscalDate,",
				"          TransactionDateTime,",
				"          TransactionEffectiveDate,",
				"          TransactionTypeCode,",
				"          TransactionType,",
				"          TransactionStatusCode,",
				"          TransactionStatus,",
				"          TransactionDescription,",
				"          PrincipalAmount,",
				"          PrincipalBalance,",
				"          LoanInterestAmount,",
				"          CHECKNBR",
				"     )) ~> sink1"
			]
		}
	}
}